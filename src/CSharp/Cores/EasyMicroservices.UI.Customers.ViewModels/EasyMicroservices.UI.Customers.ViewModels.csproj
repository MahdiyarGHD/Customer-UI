<Project Sdk="Microsoft.NET.Sdk">
  
  <PropertyGroup>
    <TargetFrameworks>net6.0;net7.0</TargetFrameworks>
    <Authors>EasyMicroservices</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.0.0.1</Version>
    <Description>Customers view model.</Description>
    <Copyright>EasyMicroservices@gmail.com</Copyright>
    <PackageTags>mvvm,viewmodel,viewmodels,modelviewviewmodel,ui,core,customers,address,person,visa,passport,phone,email,customer</PackageTags>
    <PackageProjectUrl>https://github.com/EasyMicroservices/Customers-UI</PackageProjectUrl>
    <LangVersion>latest</LangVersion>
    <DocumentationFile>.\bin\$(Configuration)\$(TargetFramework)\EasyMicroservices.UI.Customers.ViewModels.xml</DocumentationFile>
    <RootNamespace>EasyMicroservices.UI.Customers</RootNamespace>
  </PropertyGroup>
  
  <PropertyGroup>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="EasyMicroservices.CustomerMicroservice.Clients" Version="0.0.0.6" />
    <PackageReference Include="EasyMicroservices.UI.Cores.Mvvm" Version="0.0.0.7" />
  </ItemGroup>

</Project>
